<li 
    class="<?PHP if($this->error==1) : ?> error<?PHP endif; ?> 
    <?PHP if($this->unread>0) : ?> unread<?PHP endif; ?> 
    <?PHP if($this->multimedia>0) : ?> multimedia<?PHP else : ?> message<?PHP endif; ?>
    <?PHP if('cat'.$this->id==Zend_Registry::get('session')->selected) : ?> active<?PHP endif; ?> 
    prio<?PHP echo $this->priority ?>"
    alt="<?PHP echo $this->lastrefresh ?>"
    id="feed_<?PHP echo $this->id ?>">
    <div class="feed-img">
        <?PHP if(Zend_Registry::get('config')->cache->enable==1 && Zend_Registry::get('config')->cache->iconcaching==1) : ?>
        <div style="background:url(<?PHP echo Zend_Registry::get('config')->favicons->url . Zend_Registry::get('config')->cache->icons?>);
                    background-position:0 -<?PHP echo $this->iconposition*16?>px;
                    margin-right:5px;
                    width:16px;
                    height:16px"></div>
        <?PHP else : ?>
        <img 
            src="<?PHP echo Zend_Registry::get('config')->favicons->url . $this->icon ?>" 
            alt="<?PHP echo $this->name; ?>" /> 
        <?PHP endif; ?>
    </div>
    <a class="feed"><?PHP echo $this->name; ?> <span><?PHP echo $this->unread > 0 ? '(' . $this->unread . ')' : ''; ?></span></a>
    <span class="prio"><?PHP echo $this->priority ?></span>
    <a class="edit" title="<?PHP echo $this->translate('edit'); ?>"><span><?PHP echo $this->translate('edit'); ?></span></a>
</li>